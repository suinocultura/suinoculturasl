# Exemplo de arquivo secrets.toml para o Streamlit Community Cloud
# Renomeie para secrets.toml quando for fazer o upload para o Streamlit Cloud
# Não inclua este arquivo no repositório do GitHub

# Credenciais para GitHub (se necessário)
[github]
token = "seu_token_github_aqui"
username = "seu_usuario_github"
repo_name = "nome_do_repositorio"
repo_owner = "dono_do_repositorio"

# Credenciais de administrador padrão
[admin]
username = "admin"
password = "senha_segura_aqui"

# Configurações adicionais específicas da aplicação
[app_settings]
ambiente = "producao"